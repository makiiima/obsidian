#include <stdio.h>

TYPEDEF STRUCT {
    INT x;
    INT y;
} Point;

INT main() {
    CONST INT MAX_NUM = 10;
    REGISTER INT i = 0;
    UNSIGNED INT j = 0;
    VOLATILE INT k = 0;
    LONG l = 0L;
    UNSIGNED LONG m = 0UL;
    FLOAT f = 3.14f;
    DOUBLE d = 2.71828;
    Point p = {1, 2};
    ENUM {A, B, C} enum_var;
    TYPEDEF INT Int;
    Int int_var = 100;
    CHAR c = 'a';
    UNSIGNED CHAR uc = 'b';
    SHORT s = 32767;
    UNSIGNED SHORT us = 65535;

    WHILE (i < MAX_NUM) {
        j += i;
        k++;
        l--;
        m *= 2;
        i++;
    }

    printf("j=%u, k=%d, l=%ld, m=%lu\n", j, k, l, m);
    printf("f=%.2f, d=%.6lf\n", f, d);
    printf("p=(%d, %d)\n", p.x, p.y);
    printf("enum_var=%d, int_var=%d\n", enum_var, int_var);
    printf("c='%c', uc='%c', s=%hd, us=%hu\n", c, uc, s, us);

    RETURN 0;
}
