#include <stdio.h>

typedef struct {
    int x;
    int y;
} Point;

int main() {
    const int MAX_NUM = 10;
    register int i = 0;
    unsigned int j = 0;
    volatile int k = 0;
    long l = 0L;
    unsigned long m = 0UL;
    float f = 3.14f;
    double d = 2.71828;
    Point p = {1, 2};
    enum {A, B, C} enum_var;
    typedef int Int;
    Int int_var = 100;
    char c = 'a';
    unsigned char uc = 'b';
    short s = 32767;
    unsigned short us = 65535;

    while (i < MAX_NUM) {
        j += i;
        k++;
        l--;
        m *= 2;
        i++;
    }

    printf("j=%u, k=%d, l=%ld, m=%lu\n", j, k, l, m);
    printf("f=%.2f, d=%.6lf\n", f, d);
    printf("p=(%d, %d)\n", p.x, p.y);
    printf("enum_var=%d, int_var=%d\n", enum_var, int_var);
    printf("c='%c', uc='%c', s=%hd, us=%hu\n", c, uc, s, us);

    return 0;
}
